{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "routes": [
        {
          "src": "/api/status",
          "dest": "/server.js",
          "methods": ["GET"]
        },
        {
          "src": "/",
          "dest": "/server.js",
          "methods": ["GET"]
        }
      ]
    }
  ]
}
